{% extends "base.jinja2" %}


{%block page%} - Error{%endblock%}

{% block content %}

<div class="column is-6 is-offset-3">
	<div class="box">
	<article class="media">
		<div class="media-left">
		<figure class="image is-128x128">
			<img src="{{url_for('static', filename='banannas.png')}}" alt="Image">
		</figure>
		</div>
		<div class="media-content">
		<div class="content has-text-centered">
			<p class="is-size-2" style="margin-bottom:0">
				<strong>Banannas</strong>
			</p>
			<p class="is-size-3">Something went wrong</p>
			<hr>
			<p class="is-size-4">{{error}}</p>
			</p>
		</div>
		</div>
	</article>
	</div>
</div>

{% endblock %}


